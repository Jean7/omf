driver=nl80211
interface=<%= uid %>
ssid=<%= wifi_configure.essid %>
channel=<%= wifi_configure.channel %>

<% if %w(a b g).include? wifi_configure.type %>
  hw_mode=<%= wifi_configure.type %>
<% elsif wifi_configure.type == 'n' %>
  wmm_enabled=1
  ieee80211n=1
  ht_capab=[HT20-]
  <% if wifi_configure.channel < 15 %>
    hw_mode=g
  <% else %>
    hw_mode=a
  <% end %>
<% end %>
