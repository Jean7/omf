NAME=gridservices2
INITD=/etc/init.d/$NAME

export PATH="${PATH:+$PATH:}/usr/sbin:/sbin"

if [ -e $INITD -a ! -x $INITD ] ; then
	echo "WARNING: $INITD should be executable"
	chmod 755 $INITD
fi

if [ -x $INITD ]; then
	update-rc.d $NAME2 defaults >/dev/null
	if [ -x "`which invoke-rc.d 2>/dev/null`" ]; then
		invoke-rc.d $NAME2 start || exit 0
	else
		/etc/init.d/$NAME2 start || exit 0
	fi
fi
